    "js-yaml": "^3.13.1",
    "nconf": "^0.10.0",
    "nunjucks": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.3.8",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.5",
