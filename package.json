    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^6.1.0",
    "mocha": "^6.2.0",
    "mockserver": "^3.1.0",
    "nodemon": "^1.19.1",
    "semantic-release": "^15.13.31",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.2.0",
